<div class="category_image" id="merchants_create_image">
  <div class="navbar">
    <span class="logo"><a href="/">MiniShop</a></span>

    <div class="nav_links">
      <%= link_to 'Item Index', '/items' %>
      <%= link_to "Merchant Index", '/merchants' %>
    </div>
  </div>

  <div class="category">
    <h1>Enter info for a new merchant</h1>
  </div>
</div>

<div class="display">
  <%= form_tag "/merchants", method: :post, class: 'form' do %>
    <p><%= label_tag :name %></p>
    <p><%= text_field_tag :name %></p>
    <p><%= label_tag :address %></p>
    <p><%= text_field_tag :address %></p>
    <p><%= label_tag :city %></p>
    <p><%= text_field_tag :city %></p>
    <p><%= label_tag :state %></p>
    <p><%= text_field_tag :state %></p>
    <p><%= label_tag :zip %></p>
    <p><%= text_field_tag :zip %></p>
    <p><%= submit_tag("submit", class: "button_2")%></p>
  <% end %>
</div>